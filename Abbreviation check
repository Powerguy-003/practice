print('Welcome to abbreviation test!')
keylist=list(abbreviation.keys())
for x in range(len(keylist)):
    u_input=input(keylist[x]+':')
    while u_input!=abbreviation.get('IPCA'):
        print ('try again')
        u_input=input(keylist[x]+':')
print('test complete!')   
